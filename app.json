{
  "expo": {
    "name": "Job Fair Company",
    "description": "Job Fair app for companies",
    "slug": "job-fair-company",
    "privacy": "public",
    "sdkVersion": "23.0.0",
    "version": "2.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app.png",
    "loading": {
      "backgroundImage": "./assets/splash.png",
      "hideExponentText": true
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4"
      ]
    },
    "android": {
      "package": "org.kset.jobfairCompany",
      "permissions": ["CAMERA"],
      "versionCode": 2,
      "splash": {
        "backgroundColor": "#191938",
        "resizeMode": "cover",
        "ldpi": "./assets/splash.png",
        "mdpi": "./assets/splash.png",
        "hdpi": "./assets/splash.png",
        "xhdpi": "./assets/splash.png",
        "xxhdpi": "./assets/splash.png",
        "xxxhdpi": "./assets/splash.png"
      }
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "org.kset.jobfairCompany",
      "splash": {
        "backgroundColor": "#191938",
        "resizeMode": "cover",
        "image": "./assets/splash.png"
      }
    }
  }
}
